@using Domain.Models

<MudStack Spacing="0">
    <MudStack Spacing="0">
        @if(MyOrganization is not null) {
            @ProfileContent
        }
    </MudStack>
    @NewsFeedContent
</MudStack>


@code {
    [Parameter] public RenderFragment? ProfileContent { get; set; }
    [Parameter] public MyOrganizationModel? MyOrganization { get; set; }
    [Parameter] public RenderFragment? NewsFeedContent { get; set; }
}
